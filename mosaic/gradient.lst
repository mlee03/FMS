     77 !#####################################################################
     78 !  NOTe: pin has halo size = 1.
     79 !  the size of pin    will be (nx+2,ny+2), T-cell center, with halo = 1
     80 !  the size of dx     will be (nx, ny+1),  N-cell center
     81 !  the size of dy     will be (nx+1, ny),  E-cell center
     82 !  the size of area   will be (nx, ny),    T-cell center.
     83 !  The size of edge_w will be (ny+1),      C-cell center
     84 !  The size of edge_e will be (ny+1),      C-cell center
     85 !  The size of edge_s will be (nx+1),      C-cell center
     86 !  The size of edge_n will be (nx+1),      C-cell center
     87 !  The size of en_n   will be (3,nx,ny+1), N-cell center
     88 !  The size of en_e   will be (3,nx+1,ny), E-cell center
     89 !  The size of vlon   will be (3,nx, ny)   T-cell center
     90 !  The size of vlat   will be (3,nx, ny),  T-cell center
     91 
     92 subroutine gradient_cubic(pin, dx, dy, area, edge_w, edge_e, edge_s, edge_n,    &
     93                           en_n, en_e, vlon, vlat, grad_x, grad_y, on_west_edge, &
     94                           on_east_edge, on_south_edge, on_north_edge)
     95 
     96   real,    dimension(:,:  ), intent(in ) :: pin, dx, dy, area
     97   real,    dimension(:    ), intent(in ) :: edge_w, edge_e, edge_s, edge_n
     98   real,    dimension(:,:,:), intent(in ) :: en_n, en_e
     99   real,    dimension(:,:,:), intent(in ) :: vlon, vlat
    100   real,    dimension(:,:  ), intent(out) :: grad_x, grad_y
    101   logical,                   intent(in ) :: on_west_edge, on_east_edge, on_south_edge, on_north_edge
    102   integer :: nx, ny
    103 
    104 
    105   nx = size(grad_x,1)
    106   ny = size(grad_x,2)
    107 
    108   if(size(pin,1) .NE. nx+2 .OR. size(pin,2) .NE. ny+2)call mpp_error(FATAL, "gradient_mod:size of pin should be (nx+2, ny+2)
    108 ")
    109   if(size(dx,1) .NE. nx .OR. size(dx,2) .NE. ny+1 ) call mpp_error(FATAL, "gradient_mod: size of dx should be (nx,ny+1)")
    110   if(size(dy,1) .NE. nx+1 .OR. size(dy,2) .NE. ny ) call mpp_error(FATAL, "gradient_mod: size of dy should be (nx+1,ny)")
    111   if(size(area,1) .NE. nx .OR. size(area,2) .NE. ny ) call mpp_error(FATAL, "gradient_mod: size of area should be (nx,ny)")
    112   if(size(vlon,1) .NE. 3 .OR. size(vlon,2) .NE. nx .OR. size(vlon,3) .NE. ny) &
    113           call mpp_error(FATAL, "gradient_mod: size of vlon should be (3,nx,ny)")

