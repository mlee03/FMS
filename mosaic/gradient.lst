    114   if(size(vlat,1) .NE. 3 .OR. size(vlat,2) .NE. nx .OR. size(vlat,3) .NE. ny) &
    115           call mpp_error(FATAL, "gradient_mod: size of vlat should be (3,nx,ny)")
    116   if(size(edge_w) .NE. ny+1) call mpp_error(FATAL, "gradient_mod: size of edge_w should be (ny+1)")
    117   if(size(edge_e) .NE. ny+1) call mpp_error(FATAL, "gradient_mod: size of edge_e should be (ny+1)")
    118   if(size(edge_s) .NE. nx+1) call mpp_error(FATAL, "gradient_mod: size of edge_s should be (nx+1)")
    119   if(size(edge_n) .NE. nx+1) call mpp_error(FATAL, "gradient_mod: size of edge_n should be (nx+1)")
    120   if(size(en_n,1) .NE. 3 .OR. size(en_n,2) .NE. nx .OR.  size(en_n,3) .NE. ny+1 ) &
    121        call mpp_error(FATAL, "gradient_mod:size of en_n should be (3, nx, ny+1)")
    122   if(size(en_e,1) .NE. 3 .OR. size(en_e,2) .NE. nx+1 .OR.  size(en_e,3) .NE. ny ) &
    123        call mpp_error(FATAL, "gradient_mod:size of en_e should be (3, nx+1, ny)")
    124 
    125   call grad_c2l(nx, ny, pin, dx, dy, area, edge_w, edge_e, edge_s, edge_n, en_n, en_e, vlon, vlat, &
    126                 grad_x, grad_y, on_west_edge, on_east_edge, on_south_edge, on_north_edge)
    127 
    128   return
    129 
    130 end subroutine gradient_cubic


