    133 subroutine calc_cubic_grid_info(xt, yt, xc, yc, dx, dy, area, edge_w, edge_e, edge_s, edge_n, &
    134                            en_n, en_e, vlon, vlat, on_west_edge, on_east_edge, on_south_edge, on_north_edge )
    135   real,    dimension(:,:  ), intent(in ) :: xt, yt, xc, yc
    136   real,    dimension(:,:  ), intent(out) :: dx, dy, area
    137   real,    dimension(:    ), intent(out) :: edge_w, edge_e, edge_s, edge_n
    138   real,    dimension(:,:,:), intent(out) :: en_n, en_e
    139   real,    dimension(:,:,:), intent(out) :: vlon, vlat
    140   logical,                   intent(in ) :: on_west_edge, on_east_edge, on_south_edge, on_north_edge
    141   integer :: nx, ny, nxp, nyp
    142 
    143 
    144   nx  = size(area,1)
    145   ny  = size(area,2)
    146   nxp = nx+1
    147   nyp = ny+1
    148 
    149   if(size(xt,1) .NE. nx+2 .OR. size(xt,2) .NE. ny+2 ) call mpp_error(FATAL, "gradient_mod: size of xt should be (nx+2,ny+2)"
    149 )
    150   if(size(yt,1) .NE. nx+2 .OR. size(yt,2) .NE. ny+2 ) call mpp_error(FATAL, "gradient_mod: size of yt should be (nx+2,ny+2)"
    150 )
    151   if(size(xc,1) .NE. nxp .OR. size(xc,2) .NE. nyp ) call mpp_error(FATAL, "gradient_mod: size of xc should be (nx+1,ny+1)")
    152   if(size(yc,1) .NE. nxp .OR. size(yc,2) .NE. nyp ) call mpp_error(FATAL, "gradient_mod: size of yc should be (nx+1,ny+1)")
    153   if(size(dx,1) .NE. nx .OR. size(dx,2) .NE. nyp ) call mpp_error(FATAL, "gradient_mod: size of dx should be (nx,ny+1)")
    154   if(size(dy,1) .NE. nxp .OR. size(dy,2) .NE. ny ) call mpp_error(FATAL, "gradient_mod: size of dy should be (nx+1,ny)")
    155   if(size(area,1) .NE. nx .OR. size(area,2) .NE. ny ) call mpp_error(FATAL, "gradient_mod: size of area should be (nx,ny)")
    156   if(size(vlon,1) .NE. 3 .OR. size(vlon,2) .NE. nx .OR. size(vlon,3) .NE. ny) &
    157           call mpp_error(FATAL, "gradient_mod: size of vlon should be (3,nx,ny)")
    158   if(size(vlat,1) .NE. 3 .OR. size(vlat,2) .NE. nx .OR. size(vlat,3) .NE. ny) &
    159           call mpp_error(FATAL, "gradient_mod: size of vlat should be (3,nx,ny)")
    160   if(size(edge_w) .NE. ny+1) call mpp_error(FATAL, "gradient_mod: size of edge_w should be (ny-1)")
    161   if(size(edge_e) .NE. ny+1) call mpp_error(FATAL, "gradient_mod: size of edge_e should be (ny-1)")
    162   if(size(edge_s) .NE. nx+1) call mpp_error(FATAL, "gradient_mod: size of edge_s should be (nx-1)")
    163   if(size(edge_n) .NE. nx+1) call mpp_error(FATAL, "gradient_mod: size of edge_n should be (nx-1)")
    164   if(size(en_n,1) .NE. 3 .OR. size(en_n,2) .NE. nx .OR.  size(en_n,3) .NE. nyp ) &
    165        call mpp_error(FATAL, "gradient_mod:size of en_n should be (3, nx, ny+1)")
    166   if(size(en_e,1) .NE. 3 .OR. size(en_e,2) .NE. nxp .OR.  size(en_e,3) .NE. ny ) &
    167        call mpp_error(FATAL, "gradient_mod:size of en_e should be (3, nx+1, ny)")
    168 
    169 
    170   call calc_c2l_grid_info(nx, ny, xt, yt, xc, yc, dx, dy, area, edge_w, edge_e, edge_s, edge_n, &
    171                           en_n, en_e, vlon, vlat, on_west_edge, on_east_edge, on_south_edge, on_north_edge )
    172 
    173 
    174   return
    175 
    176 end subroutine calc_cubic_grid_info

