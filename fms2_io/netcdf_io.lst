    629 !> @brief Close a netcdf file.
    630 subroutine netcdf_file_close(fileobj)
    631 
    632   class(FmsNetcdfFile_t),intent(inout) :: fileobj !< File object.
    633 
    634   integer :: err
    635   integer :: i
    636 
    637   if (fileobj%is_root) then
    638     err = nf90_close(fileobj%ncid)
    639     call check_netcdf_code(err)
    640   endif
    641   if (allocated(fileobj%is_open)) fileobj%is_open = .false.
    642   fileobj%path = missing_path
    643   fileobj%ncid = missing_ncid
    644   if (allocated(fileobj%pelist)) then
    645     deallocate(fileobj%pelist)
    646   endif
    647   fileobj%io_root = missing_rank
    648   fileobj%is_root = .false.
    649   if (allocated(fileobj%restart_vars)) then
    650     deallocate(fileobj%restart_vars)
    651   endif
    652   fileobj%is_restart = .false.
    653   fileobj%num_restart_vars = 0
    654   do i = 1, fileobj%num_compressed_dims
    655     if (allocated(fileobj%compressed_dims(i)%npes_corner)) then
    656       deallocate(fileobj%compressed_dims(i)%npes_corner)
    657     endif
    658     if (allocated(fileobj%compressed_dims(i)%npes_nelems)) then
    659       deallocate(fileobj%compressed_dims(i)%npes_nelems)
    660     endif
    661   enddo
    662   if (allocated(fileobj%compressed_dims)) then
    663     deallocate(fileobj%compressed_dims)
    664   endif
    665 end subroutine netcdf_file_close

