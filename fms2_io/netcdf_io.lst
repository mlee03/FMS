   1803 !> @brief Gathers a compressed arrays size and offset for each pe.
   1804 subroutine compressed_start_and_count(fileobj, nelems, npes_start, npes_count)
   1805 
   1806   class(FmsNetcdfFile_t), intent(in) :: fileobj !< File object.
   1807   integer, intent(in) :: nelems !< Number of elements on the current pe.
   1808   integer, dimension(:), allocatable, intent(out) :: npes_start !< Offset for each pe.
   1809   integer, dimension(:), allocatable, intent(out) :: npes_count !< Number of elements for
   1810 !! each pe.
   1811 
   1812   integer :: i
   1813 
   1814   allocate(npes_start(size(fileobj%pelist)))
   1815   allocate(npes_count(size(fileobj%pelist)))
   1816   do i = 1, size(fileobj%pelist)
   1817     if (fileobj%pelist(i) .eq. mpp_pe()) then
   1818       npes_count(i) = nelems
   1819     else
   1820       call mpp_recv(npes_count(i), fileobj%pelist(i), block=.false.)
   1821       call mpp_send(nelems, fileobj%pelist(i))
   1822     endif
   1823   enddo
   1824   call mpp_sync_self(check=EVENT_RECV)
   1825   call mpp_sync_self(check=EVENT_SEND)
   1826   npes_start(1) = 1
   1827   do i = 1, size(fileobj%pelist)-1
   1828     npes_start(i+1) = npes_start(i) + npes_count(i)
   1829   enddo
   1830 end subroutine compressed_start_and_count

