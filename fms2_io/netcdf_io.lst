   5833 !> @brief Loop through the registered restart variables (including regional
   5834 !! variables) and write them to the netcdf file
   5835 subroutine write_restart_bc(fileobj, unlim_dim_level)
   5836   class(FmsNetcdfFile_t), intent(inout) :: fileobj !< File object
   5837   integer, intent(in), optional :: unlim_dim_level !< Unlimited dimension
   5838 !! level.
   5839   integer :: i !< No description
   5840 
   5841   if (.not. fileobj%is_restart) then
   5842     call error("file "//trim(fileobj%path)//" is not a restart file.")
   5843   endif
   5844 
   5845 !> Loop through the variables, root pe gathers the data from the other pes and writes out the checksum.
   5846 !! Then loop through the variables again to write the data to the netcdf file.
   5847 !! All the metadata should be written before the data to prevent netcdf from rewriting the file
   5848 !! if the header is not big enough. That is why the two do loops are needed.
   5849 
   5850  do i = 1, fileobj%num_restart_vars
   5851 !> Go away if you are not in the pelist!
   5852     if (.not.ANY(mpp_pe().eq.fileobj%restart_vars(i)%bc_info%pelist(:))) cycle
   5853 
   5854 !> Go away if this is not a BC variable
   5855     if (.not. fileobj%restart_vars(i)%is_bc_variable) cycle
   5856 
   5857 !> Root pe gathers the data from the other ranks, saves it in a buffer, and writes out the checksum.
   5858     if (associated(fileobj%restart_vars(i)%data2d)) then
   5859         call gather_data_bc(fileobj, fileobj%restart_vars(i)%data2d, fileobj%restart_vars(i)%bc_info)
   5860         call register_variable_attribute(fileobj, fileobj%restart_vars(i)%varname, "checksum", &
   5861              fileobj%restart_vars(i)%bc_info%chksum, str_len=len(fileobj%restart_vars(i)%bc_info%chksum))
   5862     else if (associated(fileobj%restart_vars(i)%data3d)) then
   5863         call gather_data_bc(fileobj, fileobj%restart_vars(i)%data3d, fileobj%restart_vars(i)%bc_info)
   5864         call register_variable_attribute(fileobj, fileobj%restart_vars(i)%varname, "checksum", &
   5865              fileobj%restart_vars(i)%bc_info%chksum, str_len=len(fileobj%restart_vars(i)%bc_info%chksum))
   5866     endif
   5867  enddo
   5868 
   5869 !> Write the data to the netcdf file
   5870  do i = 1, fileobj%num_restart_vars
   5871     if (allocated(fileobj%restart_vars(i)%bc_info%globaldata2d_r8 )) then
   5872        call netcdf_write_data(fileobj, fileobj%restart_vars(i)%varname, &
   5873                             fileobj%restart_vars(i)%bc_info%globaldata2d_r8 , &
   5874                             unlim_dim_level=unlim_dim_level)
   5875        deallocate(fileobj%restart_vars(i)%bc_info%globaldata2d_r8)
