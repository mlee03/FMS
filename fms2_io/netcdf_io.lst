    304 !> @brief Accepts the namelist fms2_io_nml variables relevant to netcdf_io_mod
    305 subroutine netcdf_io_init (chksz, header_buffer_val, netcdf_default_format)
    306 integer, intent(in) :: chksz
    307 character (len = 10), intent(in) :: netcdf_default_format
    308 integer, intent(in) :: header_buffer_val
    309 
    310  fms2_ncchksz = chksz
    311  fms2_header_buffer_val = header_buffer_val
    312  if (string_compare(netcdf_default_format, "64bit", .true.)) then
    313      fms2_nc_format_param = nf90_64bit_offset
    314      call string_copy(fms2_nc_format, "64bit")
    315  elseif (string_compare(netcdf_default_format, "classic", .true.)) then
    316      fms2_nc_format_param = nf90_classic_model
    317      call string_copy(fms2_nc_format, "classic")
    318  elseif (string_compare(netcdf_default_format, "netcdf4", .true.)) then
    319      fms2_nc_format_param = nf90_netcdf4
    320      call string_copy(fms2_nc_format, "netcdf4")
    321  else
    322      call error("unrecognized netcdf file format "//trim(netcdf_default_format)// &
    323      '. The acceptable values are "64bit", "classic", "netcdf4". Check fms2_io_nml: netcdf_default_format')
    324  endif
    325 
    326 end subroutine netcdf_io_init

