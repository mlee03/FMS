    727 !> @brief Add a dimension to a file.
    728 subroutine netcdf_add_dimension(fileobj, dimension_name, dimension_length, &
    729                                 is_compressed)
    730 
    731   class(FmsNetcdfFile_t), intent(inout) :: fileobj !< File object.
    732   character(len=*), intent(in) :: dimension_name !< Dimension name.
    733   integer, intent(in) :: dimension_length !< Dimension length.
    734   logical, intent(in), optional :: is_compressed !< Changes the meaning of dim_len from
    735 !! referring to the total size of the
    736 !! dimension (when false) to the local
    737 !! size for the current rank (when true).
    738 
    739   integer :: dim_len
    740   integer, dimension(:), allocatable :: npes_start
    741   integer, dimension(:), allocatable :: npes_count
    742   integer :: i
    743   integer :: err
    744   integer :: dimid
    745 
    746   dim_len = dimension_length
    747   if (present(is_compressed)) then
    748     if (is_compressed) then
    749 !Gather all local dimension lengths on the I/O root pe.
    750       allocate(npes_start(size(fileobj%pelist)))
    751       allocate(npes_count(size(fileobj%pelist)))
    752       do i = 1, size(fileobj%pelist)
    753         if (fileobj%pelist(i) .eq. mpp_pe()) then
    754           npes_count(i) = dim_len
    755         else
    756           call mpp_recv(npes_count(i), fileobj%pelist(i), block=.false.)
    757           call mpp_send(dim_len, fileobj%pelist(i))
    758         endif
    759       enddo
    760       call mpp_sync_self(check=event_recv)
    761       call mpp_sync_self(check=event_send)
    762       npes_start(1) = 1
    763       do i = 1, size(fileobj%pelist)-1
    764         npes_start(i+1) = npes_start(i) + npes_count(i)
    765       enddo
    766       call append_compressed_dimension(fileobj, dimension_name, npes_start, &
    767                                        npes_count)
    768       dim_len = sum(npes_count)
    769     endif
    770   endif
    771   if (fileobj%is_root .and. .not. fileobj%is_readonly) then
    772     call set_netcdf_mode(fileobj%ncid, define_mode)
    773     err = nf90_def_dim(fileobj%ncid, trim(dimension_name), dim_len, dimid)
    774     call check_netcdf_code(err)
    775   endif
    776 end subroutine netcdf_add_dimension

