    343 !> @brief Switch to the correct netcdf mode.
    344 !! @internal
    345 subroutine set_netcdf_mode(ncid, mode)
    346 
    347   integer, intent(in) :: ncid !< Netcdf file id.
    348   integer, intent(in) :: mode !< Netcdf file mode.
    349 
    350   integer :: err
    351 
    352   if (mode .eq. define_mode) then
    353     err = nf90_redef(ncid)
    354     if (err .eq. nf90_eindefine .or. err .eq. nf90_eperm) then
    355       return
    356     endif
    357   elseif (mode .eq. data_mode) then
    358     if (fms2_header_buffer_val == -1) call error("set_netcdf_mode: fms2_header_buffer_val not set, call fms2_io_init")
    359     err = nf90_enddef(ncid, h_minfree=fms2_header_buffer_val)
    360     if (err .eq. nf90_enotindefine .or. err .eq. nf90_eperm) then
    361       return
    362     endif
    363   else
    364     call error("mode must be either define_mode or data_mode.")
    365   endif
    366   call check_netcdf_code(err)
    367 end subroutine set_netcdf_mode


