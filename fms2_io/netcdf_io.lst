   1768 !> @brief Determine if a piece of data is "valid" (in the correct range.)
   1769 !! @return A flag telling if the data element is "valid."
   1770 elemental function check_if_valid(rdatum, validobj) &
   1771   result(valid_data)
   1772 
   1773   real(kind=r8_kind), intent(in) :: rdatum !< packed data element.
   1774   type(Valid_t), intent(in) :: validobj !< Valid object.
   1775   logical :: valid_data
   1776 
   1777   valid_data = .true.
   1778 ! If the variable has a range (open or closed), valid values must be in that
   1779 ! range.
   1780   if (validobj%has_range) then
   1781     if (validobj%has_min .and. .not. validobj%has_max) then
   1782       valid_data = rdatum .ge. validobj%min_val
   1783     elseif (validobj%has_max .and. .not. validobj%has_min) then
   1784       valid_data = rdatum .le. validobj%max_val
   1785     else
   1786       valid_data = .not. (rdatum .lt. validobj%min_val .or. rdatum .gt. validobj%max_val)
   1787     endif
   1788   endif
   1789 ! If the variable has a fill value or missing value, valid values must not be
   1790 ! equal to either.
   1791   if (validobj%has_fill .or. validobj%has_missing) then
   1792     if (validobj%has_fill .and. .not. validobj%has_missing) then
   1793       valid_data = rdatum .ne. validobj%fill_val
   1794     elseif (validobj%has_missing .and. .not. validobj%has_fill) then
   1795       valid_data = rdatum .ne. validobj%missing_val
   1796     else
   1797       valid_data = .not. (rdatum .eq. validobj%missing_val .or. rdatum .eq. validobj%fill_val)
   1798     endif
   1799   endif
   1800 end function check_if_valid

