    690 !> @brief Add a compressed dimension to a file object.
    691 !! @internal
    692 subroutine append_compressed_dimension(fileobj, dim_name, npes_corner, &
    693                                        npes_nelems)
    694 
    695   class(FmsNetcdfFile_t), intent(inout) :: fileobj !< File object.
    696   character(len=*), intent(in) :: dim_name !< Dimension name.
    697   integer, dimension(:), intent(in) :: npes_corner !< Array of starting
    698 !! indices for each rank.
    699   integer, dimension(:), intent(in) :: npes_nelems !< Number of elements
    700 !! associated with each
    701 !! rank.
    702 
    703   integer :: n
    704 
    705   if (get_compressed_dimension_index(fileobj, dim_name) .ne. dimension_not_found) then
    706     call error("dimension "//trim(dim_name)//" already registered" &
    707                //" to file "//trim(fileobj%path)//".")
    708   endif
    709   fileobj%num_compressed_dims = fileobj%num_compressed_dims + 1
    710   n = fileobj%num_compressed_dims
    711   if (n .gt. max_num_compressed_dims) then
    712     call error("number of compressed dimensions exceeds limit.")
    713   endif
    714   call string_copy(fileobj%compressed_dims(n)%dimname, dim_name)
    715   if (size(npes_corner) .ne. size(fileobj%pelist) .or. &
    716       size(npes_nelems) .ne. size(fileobj%pelist)) then
    717     call error("incorrect size for input npes_corner or npes_nelems arrays.")
    718   endif
    719   allocate(fileobj%compressed_dims(n)%npes_corner(size(fileobj%pelist)))
    720   fileobj%compressed_dims(n)%npes_corner(:) = npes_corner(:)
    721   allocate(fileobj%compressed_dims(n)%npes_nelems(size(fileobj%pelist)))
    722   fileobj%compressed_dims(n)%npes_nelems(:) = npes_nelems(:)
    723   fileobj%compressed_dims(n)%nelems = sum(fileobj%compressed_dims(n)%npes_nelems)
    724 end subroutine append_compressed_dimension

