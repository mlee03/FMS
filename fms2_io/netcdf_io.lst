   5629 !> @brief Returns a variable's fill value if it exists in the file.
   5630 !! @return Flag telling if a fill value exists.
   5631 function get_fill_value(fileobj, variable_name, fill_value, broadcast) &
   5632   result(fill_exists)
   5633 
   5634   class(FmsNetcdfFile_t), intent(in) :: fileobj !< File object.
   5635   character(len=*), intent(in) :: variable_name !< Variable name.
   5636   class(*), intent(out) :: fill_value !< Fill value.
   5637   logical, intent(in), optional :: broadcast !< Flag controlling whether or
   5638 !! not the data will be
   5639 !! broadcasted to non
   5640 !! "I/O root" ranks.
   5641 !! The broadcast will be done
   5642 !! by default.
   5643   logical :: fill_exists
   5644 
   5645   character(len=32), dimension(2) :: attribute_names
   5646   logical :: bcast
   5647   integer :: i
   5648 
   5649   fill_exists = .false.
   5650   call string_copy(attribute_names(1), "_FillValue")
   5651   call string_copy(attribute_names(2), "missing_value")
   5652   if (present(broadcast)) then
   5653     bcast = broadcast
   5654   else
   5655     bcast = .true.
   5656   endif
   5657   do i = 1, size(attribute_names)
   5658     fill_exists = variable_att_exists(fileobj, variable_name, attribute_names(i), &
   5659                                       broadcast=bcast)
   5660     if (fill_exists) then
   5661       call get_variable_attribute(fileobj, variable_name, attribute_names(i), &
   5662                                   fill_value, broadcast=bcast)
   5663       exit
   5664     endif
   5665   enddo
