    114 pure subroutine cld_ntrp_linear_cell_interp(fvals, ts, f, ier)
    115     real, intent(in) :: fvals(0:)  !< values at the cell nodes
    116     real, intent(in) :: ts(:)      !< normalized [0,1]^nd cell coordinates
    117     real, intent(out):: f          !< interpolated value
    118     integer, intent(out) ::  ier   !< error flag (0=ok)
    119 
    120     integer j, nd, Ic, iflag
    121     integer ie(size(fvals))
    122     real    basis
    123 
    124     ier = 0
    125     f   = 0.
    126     nd   = size(ts)
    127     if(size(fvals) /= 2**nd) then
    128        ier = 1
    129        return
    130     endif
    131 
    132     do Ic = 0, 2**nd - 1
    133        basis = 1.
    134        call cld_ntrp_expand_index(Ic, ie, iflag)
    135        do j = 1, nd
    136           basis = basis * (  (1.0-real(ie(j)))*(1.0-ts(j)) + real(ie(j))*ts(j) )
    137        end do
    138        f = f + fvals(Ic)*basis
    139     end do
    140 
    141   end subroutine cld_ntrp_linear_cell_interp

