    145 pure subroutine cld_ntrp_locate_cell(axis, x, index, ier)
    146     real, intent(in)     :: axis(:) !< axis
    147     real, intent(in)     :: x       !< abscissae
    148     integer, intent(out) :: index   !< lower-left corner index
    149     integer, intent(out) ::  ier    !< error flag (0=ok)
    150 
    151     logical down
    152     integer n, index1, is
    153     real axis_1, axis_n, axis_min, axis_max
    154     ier   = 0
    155     index = -1
    156     down = .FALSE.
    157     n = size(axis)
    158     if(n < 2) then
    159        ier = 3
    160        return
    161     endif
    162     axis_1 = axis(1)
    163     axis_n = axis(n)
    164     axis_min = axis_1
    165     axis_max = axis_n
    166     if(axis_1 > axis_n) then
    167        down = .TRUE.
    168        axis_min = axis_n
    169        axis_max = axis_1
    170     endif
    171 
    172     if(x < axis_min-tol) then
    173        ier = 1
    174        return
    175     endif
    176     if(x > axis_max+tol) then
    177        ier = 2
    178        return
    179     endif
    180 
    181     index = floor(real(n-1)*(x - axis_1)/(axis_n-axis_1)) + 1
    182     index  = min(n-1, index)
    183     index1 = index+1
    184 
    185     if(.NOT. down) then
    186        if(axis(index) <= x+tol) then
    187           if(x <= axis(index1)+tol) then
    188 ! axis is uniform, or nearly so. Done!
    189              return
    190           else
    191 ! increase index
    192              is = index+1
    193              do index = is, n-1
    194                 index1 = index+1
    195                 if(axis(index1) >= x-tol) return
    196              enddo
    197           endif
    198        else

