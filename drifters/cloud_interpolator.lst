     67 pure subroutine cld_ntrp_expand_index(Ic, ie, ier)
     68     integer, intent(in)  ::  Ic    !< contacted index
     69     integer, intent(out) ::  ie(:) !< expanded list of indices
     70     integer, intent(out) ::  ier   !< error flag (0=ok)
     71 
     72     integer j, nd
     73 
     74     ier =  0
     75     nd  = size(ie) ! dimension
     76 
     77     if(Ic >= 2**nd) then
     78        ie  = -1
     79        ier = 1 ! error
     80        return
     81     endif
     82 
     83     do j = 1, nd
     84        ie(j) = mod(Ic/2**(j-1), 2)
     85     end do
     86 
     87   end subroutine cld_ntrp_expand_index

