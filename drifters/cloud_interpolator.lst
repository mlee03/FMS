    203           enddo
    204        endif
    205     else
    206 ! axis is pointing down
    207        if(axis(index) >= x-tol) then
    208           if(x >= axis(index1)-tol) then
    209 ! axis is uniform, or nearly so. Done!
    210              return
    211           else
    212 ! increase index
    213              is = index + 1
    214              do index = is, n-1
    215                 index1 = index+1
    216                 if(axis(index1) <= x+tol) return
    217              enddo
    218           endif
    219        else
    220 ! decrease index
    221           is = index - 1
    222           do index = is, 1, -1
    223              if(axis(index) >= x-tol) return
