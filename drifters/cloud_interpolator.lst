    232 pure subroutine cld_ntrp_get_flat_index(nsizes, indices, flat_index, ier)
    233     integer, intent(in)  :: nsizes(:)  !< size of array along each axis
    234     integer, intent(in)  :: indices(:) !< cell indices
    235     integer, intent(out) :: flat_index !< index into flattened array
    236     integer, intent(out) ::  ier       !< error flag (0=ok)
    237 
    238     integer nd, id
    239 
    240     ier = 0
    241     flat_index = -1
    242     nd = size(nsizes)
    243     if(nd /= size(indices)) then
    244 ! size mismatch
    245        ier = 1
    246        return
    247     endif
    248 
    249     flat_index = indices(nd)-1
    250     do id = nd-1, 1, -1
    251        flat_index = flat_index*nsizes(id) + indices(id)-1
    252     enddo
    253     flat_index = flat_index + 1
    254 
    255   end subroutine cld_ntrp_get_flat_index


