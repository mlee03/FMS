    259 pure subroutine cld_ntrp_get_cell_values(nsizes, fnodes, indices, fvals, ier)
    260     integer, intent(in)  :: nsizes(:)  !< size of fnodes along each axis
    261     real, intent(in)     :: fnodes(:)  !< flattened array of node values
    262     integer, intent(in)  :: indices(:) !< cell indices
    263     real, intent(out)    :: fvals(0:)  !< returned array values in the cell
    264     integer, intent(out) ::  ier       !< error flag (0=ok)
    265 
    266     integer id, nt, nd, flat_index, Ic, iflag
    267     integer, dimension(size(nsizes)) :: cell_indices, node_indices
    268     ier = 0
    269     fvals = 0.
    270 
    271     nd = size(nsizes)
    272     if(nd /= size(indices)) then
    273 ! size mismatch
    274        ier = 1
    275        return
    276     endif
    277     if(2**nd > size(fvals)) then
    278 ! not enough elements to hold result
    279        ier = 2
    280        return
    281     endif
    282     nt = 1
    283     do id = 1, nd
    284        nt = nt * nsizes(id)
    285     enddo
    286     if(nt /= size(fnodes)) then
    287 ! not enough node values
    288        ier = 3
    289        return
    290     endif
    291 
    292     do Ic = 0, 2**nd-1
    293        call cld_ntrp_expand_index(Ic, cell_indices, iflag)
    294        node_indices = indices + cell_indices
    295        call cld_ntrp_get_flat_index(nsizes, node_indices, flat_index, iflag)
    296        fvals(Ic) = fnodes(flat_index)
    297     enddo
    298 
    299   end subroutine cld_ntrp_get_cell_values

