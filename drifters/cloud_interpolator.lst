     91 pure subroutine cld_ntrp_contract_indices(ie, Ic, ier)
     92     integer, intent(in) ::  ie(:)  !< expanded list of indices
     93     integer, intent(out)  ::  Ic   !< contacted index
     94     integer, intent(out) ::  ier   !< error flag (0=ok)
     95 
     96     integer j, nd
     97 
     98     ier = 0
     99     nd  = size(ie) ! dimension
    100 
    101     Ic = ie(nd)
    102     do j = nd-1, 1, -1
    103        Ic = Ic * 2
    104        Ic = Ic + ie(j)
    105     end do
    106 
    107     if(Ic >= 2**nd) ier = 1
    108 
    109   end subroutine cld_ntrp_contract_indices


