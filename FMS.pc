prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

# Including the following, as they may be useful when building other components
# and as a way to better record how the library was built.
ccompiler=cc
fccompiler=ftn
cppflagss.compiled=-DINTERNAL_FILE_NML -DCLUBB -Duse_netCDF -DHAVE_SCHED_GETAFFINITY -Duse_LARGEFILE -D__IFC -fopenmp
cflags.compiled=-g -O0 -debug -DpgiFortran -fopenmp
fcflags.compiled=-O0 -g -debug -Fno-alias -auto -safe-cray-ptr -ftz -assume byterecl -i4 -r8 -nowarn -sox -traceback -qopenmp  -FR 
ldflags.compiled=-I/lustre/f2/dev/Mikyung.Lee/FMS/FMS2022.01/include -L/lustre/f2/dev/Mikyung.Lee/FMS/FMS2022.01/lib

Name: FMS
Description: The Flexible Modeling System Infrastructure Library
URL: https://www.gfdl.noaa.gov/fms
Version: 2022.03.00
Libs: -L$(libdir) -lFMS
Libs.private: 
Cflags: -I${includedir}
Fflags: -I${includedir}
