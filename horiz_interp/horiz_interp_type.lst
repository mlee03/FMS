    114 !#######################################################################
    115 !> @brief This statistics is for bilinear interpolation and spherical regrid.
    116  subroutine stats ( dat, low, high, avg, miss, missing_value, mask )
    117  real,    intent(in)  :: dat(:,:)
    118  real,    intent(out) :: low, high, avg
    119  integer, intent(out) :: miss
    120  real, intent(in), optional :: missing_value
    121  real,    intent(in), optional :: mask(:,:)
    122 
    123  real :: dsum, npts, buffer_real(3)
    124  integer :: pe, root_pe, npes, p, buffer_int(2)
    125 
    126    pe = mpp_pe()
    127    root_pe = mpp_root_pe()
    128    npes = mpp_npes()
    129 
    130    dsum = 0.0
    131    miss = 0
    132 
    133    if (present(missing_value)) then
    134       miss = count(dat(:,:) == missing_value)
    135       low  = minval(dat(:,:), dat(:,:) /= missing_value)
    136       high = maxval(dat(:,:), dat(:,:) /= missing_value)
    137       dsum = sum(dat(:,:), dat(:,:) /= missing_value)
    138    else if(present(mask)) then
    139       miss = count(mask(:,:) <= 0.5)
    140       low  = minval(dat(:,:),mask=mask(:,:) > 0.5)
    141       high = maxval(dat(:,:),mask=mask(:,:) > 0.5)
    142       dsum = sum(dat(:,:), mask=mask(:,:) > 0.5)
    143    else
    144       miss = 0
    145       low  = minval(dat(:,:))
    146       high = maxval(dat(:,:))
    147       dsum = sum(dat(:,:))
    148    endif
    149    avg = 0.0
    150 
    151    npts = size(dat(:,:)) - miss
    152    if(pe == root_pe) then
    153       do p = 1, npes - 1  ! root_pe receive data from other pe
    154 ! Force use of "scalar", integer pointer mpp interface
    155          call mpp_recv(buffer_real(1),glen=3, from_pe=p+root_pe, tag=COMM_TAG_1)
    156          dsum = dsum + buffer_real(1)
    157          low  = min(low, buffer_real(2))
    158          high = max(high, buffer_real(3))
    159          call mpp_recv(buffer_int(1), glen=2, from_pe=p+root_pe, tag=COMM_TAG_2)
    160          miss = miss + buffer_int(1)
    161          npts = npts + buffer_int(2)
    162       enddo
    163       if(npts == 0.) then
    164          print*, 'Warning: no points is valid'
    165       else
    166          avg = dsum/real(npts)
    167       endif
    168     else   ! other pe send data to the root_pe.
    169       buffer_real(1) = dsum

Page 4           Source Listing                  STATS
2021-06-01 08:55                                 horiz_interp_type.F90

    170       buffer_real(2) = low
    171       buffer_real(3) = high
    172 ! Force use of "scalar", integer pointer mpp interface
    173       call mpp_send(buffer_real(1),plen=3,to_pe=root_pe, tag=COMM_TAG_1)
    174       buffer_int(1) = miss
    175       buffer_int(2) = npts
    176       call mpp_send(buffer_int(1), plen=2, to_pe=root_pe, tag=COMM_TAG_2)
    177     endif
    178 
    179     call mpp_sync_self()
    180 
    181     return
    182 
    183  end subroutine stats


